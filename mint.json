{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Osmosis API",
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "favicon": "/favicon.png",
  "openapi": "/openapi.json",
  "api": {
    "baseUrl": "https://osmosis.gulp.dev",
    "playground": {
      "mode": "hide"
    },
    "auth": {
      "method": "key",
      "name": "x-api-key"
    },
    "maintainOrder": true
  },
  "colors": {
    "primary": "#707FDD",
    "light": "#8892E3",
    "dark": "#5C6AD4",
    "anchors": {
      "from": "#707FDD",
      "to": "#8892E3"
    }
  },
  "topbarLinks": [
    {
      "name": "Contact Us",
      "url": "mailto:founders@gulp.ai"
    }
  ],
  "tabs": [
    {
      "name": "Examples",
      "url": "examples"
    },
    {
      "name": "Python SDK",
      "url": "python-sdk"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": [
        "introduction"
      ]
    },
    {
      "group": "Python SDK",
      "pages": [
        "python-sdk/introduction",
        "python-sdk/installation",
        "python-sdk/quickstart",
        "python-sdk/api-reference",
        "python-sdk/cli-quickstart",
        "python-sdk/cli-reference",
        "python-sdk/decorators-api"
      ]
    }
  ]
}
